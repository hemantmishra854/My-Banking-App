<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bankingapp">

    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Banking">
        <activity android:name=".AccountActivity"></activity>
        <activity android:name=".OfferActivity" />
        <activity
            android:name=".FundTransferActivity"
            android:parentActivityName=".WelcomeActivity" />
        <activity
            android:name=".ContactUsActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".FeedbackActivity"
            android:parentActivityName=".WelcomeActivity" />
        <activity android:name=".CardServicesActivity" />
        <activity
            android:name=".ProfileActivity"
            android:parentActivityName=".WelcomeActivity" />
        <activity android:name=".MainCardsActivity" />
        <activity android:name=".MainDepositActivity" />
        <activity android:name=".MainLoanActivity" />
        <activity
            android:name=".OpenAccountActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".CustomerLoginActivity" />
        <activity
            android:name=".DepositActivity"
            android:parentActivityName=".WelcomeActivity" />
        <activity
            android:name=".LoanActivity"
            android:parentActivityName=".WelcomeActivity" />
        <activity android:name=".WelcomeActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>